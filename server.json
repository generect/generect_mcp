{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-07-09/server.schema.json",
  "name": "io.github.generect/generect-mcp",
  "description": "B2B lead generation, company search, and email finding through Generect Live API. Find prospects by job title, location, industry, and company filters.",
  "version": "0.1.0",
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://generect-mcp.onrender.com/mcp",
      "headers": [
        {
          "name": "Authorization",
          "description": "Bearer Token YOUR_GENERECT_API_KEY from https://app.generect.com",
          "is_required": true,
          "is_secret": true
        }
      ]
    }
  ]
}
